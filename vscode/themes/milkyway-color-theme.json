  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4a5568"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#c2c8d1"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#8a7aa9"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8a7aa9"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace", "entity.name.scope-resolution"],
      "settings": {
        "foreground": "#52796f"
      }
    },
    {
      "name": "Methods",
      "scope": ["meta.method", "meta.method-call"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#52796f"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e79214"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d03535"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#8a7aa9"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#52796f"
      }
    },
    {
      "name": "Markup - Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#8a7aa9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#e79214",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#8a7aa9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup - Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d03535"
      }
    }
  ]
}
